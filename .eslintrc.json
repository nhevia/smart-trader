{
  "extends": [
    "prettier",
    "prettier/standard",
    "prettier/react",
    "plugin:react/recommended"
  ],
  "plugins": ["prettier", "react", "react-hooks", "@babel"],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 0
  },
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  }
}
